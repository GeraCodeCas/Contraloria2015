//Inicializacion de la Clase
  public Anexo3X() throws SQLException, IOException {
    String sSql = "";
    iIdRenglon = 0;
    iIdFuncionario = 0;
    sClave = "";
    sPrograma = "";
    sMunicipio = "";
    fMontoAprobado = "0";
    fMontoEjercido = "0";
    sFechaInicial = "";
    sFechaFinal = "";
    sAvanceFisico = "";
    sAvanceFinanciero = "";
    sObservacion = "";
    iTipoOrigen = 0;
    iTipoNvo = 0;
    sMunicipioDesc = "";
    oVm = new VMatrix();
    oVm2 = new VMatrix();
    oCnn = new ConnectionJDBC();
    iGetAllCount = 0;

    oCnn.Open();
    ResultSet oRs = null;
    sSql =
        "SELECT IdAnexo, Clave, Nombre FROM Anexo WHERE Clave = 'Anexo III.10'";
    try {
      oRs = oCnn.Execute(sSql);
      if (oRs.next()) {
        iIdAnexo = oRs.getInt("IdAnexo");
        sNombreAnexo = oRs.getString("Nombre");
      }
      oRs.close();
      oRs = null;
    }
    catch (Throwable e) {
      System.out.println("Error en oAnexo3VII.init: " + e + " SQL:" + sSql);
    }
    finally {
      if (oCnn != null) {
        oCnn.Close();
      }
    }
  }

