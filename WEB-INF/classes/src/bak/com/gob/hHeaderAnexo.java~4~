              sLinkAttach + iIdAnexo + "\\" + sArchivos +
              "'><font color='Blue'>" + sArchivos + "</font></a>";
          bBandera = true;
        }
        oArchivoA.Clear();
        oArchivowa.Escribe(sArchivo);
        sArchivo = "	</TR>";
        oArchivowa.Escribe(sArchivo);
      }
      sArchivo = "	</TABLE><!--De attachment-->";
      oArchivo.Escribe(sArchivo);
